[Tests]
  # Syntax checks
  [generate_mesh]
    type = RunApp
    executable_pattern = 'griffin*|pronghorn*'
    input = '../mesh/mesh.i'
    cli_args = '--mesh-only'
    working_directory = '../mesh'
  []
  [neu_msre_syntax]
    type = RunApp
    input = 'neu.i'
    check_input = True
    executable_pattern = 'griffin*'
    cli_args = "MultiApps/active='' Transfers/active=''"
  []
  [th_msre_syntax]
    type = RunApp
    input = 'th.i'
    check_input = True
    executable_pattern = 'pronghorn*'
  []
  # Regression tests with CSV output over a minimum amount of time steps/computation
  [neu_msre]
    prereq = neu_msre_syntax
    type = CSVDiff
    rel_err = 1.0E-4
    input = 'neu.i'
    csvdiff = 'msre_neutronics_ss_s2_out.csv'
    executable_pattern = 'griffin*'
    allow_warnings = true
    cli_args = "MultiApps/active='' Transfers/active=''"
    method = 'opt'
  []
  [th_msre]
    prereq = th_msre_syntax
    type = CSVDiff
    rel_err = 1.0E-4
    input = 'th.i'
    csvdiff = 'th_out.csv'
    executable_pattern = 'pronghorn*'
    allow_warnings = true
    method = 'opt'
    min_parallel = 8
  []
[]
