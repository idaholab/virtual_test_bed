    check_files = abr_7assemblies_in.e
    cli_args = "--mesh-only"
  []
  # Regression checks for 7-assembly SCM center model
  [SCM7]
    type = CSVDiff
    input = HC_master.i
    csvdiff = HC_master_out_subchannel0.csv
    capabilities = 'method!=dbg'
    valgrind = NONE
    recover = false
    abs_zero = 1e-5
    max_threads = 1
    max_parallel = 1
    requirement = 'The system shall be able to solve a 7-assembly case using the subchannel solver'
    prereq = 'generate_mesh'
    executable_pattern = 'subchannel*|pronghorn*|combined*'
  []
[]
