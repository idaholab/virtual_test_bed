[Tests]
  # Syntax checks for 7-assembly SCM center model
  [Master_app_syntax]
    type = RunApp
    input = HC_master.i
    check_input = True
    prereq = 'generate_mesh'
    executable_pattern = 'subchannel*|pronghorn*|combined*'
  []
  [SCM_syntax]
    type = RunApp
    input = fuel_assembly_center.i
    check_input = True
    executable_pattern = 'subchannel*|pronghorn*|combined*'
  []
  [SCM_viz_syntax]
    type = RunApp
    input = 3d.i
    check_input = True
    executable_pattern = 'subchannel*|pronghorn*|combined*'
  []
  [generate_mesh]
    type = CheckFiles
    input = abr_7assemblies.i
    check_files = abr_7assemblies_in.e
    cli_args = "--mesh-only"
    executable_pattern = 'subchannel*|pronghorn*|combined*'
  []
  # Regression checks for 7-assembly SCM models
  [SCM7]
    type = CSVDiff
    input = HC_master.i
    csvdiff = 'HC_master_out.csv HC_master_out_subchannel0.csv'
    capabilities = 'method!=dbg'
    valgrind = NONE
    recover = false
    abs_zero = 1e-5
    max_threads = 1
    cli_args = 'Executioner/fixed_point_min_its=3 Executioner/fixed_point_max_its=3'
    min_parallel = 7
    rel_err = 5e-3
    allow_warnings = true
    requirement = 'The system shall be able to solve a 7-assembly case using the subchannel solver'
    prereq = 'generate_mesh'
    executable_pattern = 'subchannel*|pronghorn*|combined*'
    max_time = 1000
  []
[]
